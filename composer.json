{
    "name": "gebit/omnipay-stripe",
    "type": "library",
    "description": "Stripe driver for the Omnipay payment processing library",
    "homepage": "https://github.com/thephpleague/omnipay-stripe",
    "license": "MIT",
    "autoload": {
        "psr-4": { "Omnipay\\Stripe\\" : "src/" }
    },
    "require": {
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^0.14"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "prefer-stable": true
}
